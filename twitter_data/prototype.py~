
import numpy as np
import pandas as pd
import re
import argparse

if __name__ == '__main__':


    for line in open('data/uncleansed/nytimes_2020-01-26_2020-02-10.txt', "r", encoding='latin').read().split('<|endoftext|>'):
        S2_line = []
        domain = 'twitter'
        date = re.search('\<\|begindate\|\>(.*)\<\|endofdate\|\>', line)
        author = 'The New York Times'
        title = re.search('\<\|begintitle\|\>(.*)\<\|endoftitle\|\>', line)
        line = re.sub('\n', ' ', line)
        article = re.search('\<\|beginarticle\|\>(.*|\n*)\<\|endofarticle\|\>', line)

        if article:
            print("line regex:", article.group(1))
        else:
            print("error:", line)

